{
   "archive" : {
      "exclude" : [
         ".*",
         "*~",
         "vendor/",
         "Makefile",
         "composer.lock",
         "composer.phar",
         "composer.yaml"
      ]
   },
   "authors" : [
      {
         "email" : "zurborg@cpan.org",
         "homepage" : "https://zurborg.github.io/",
         "name" : "David Zurborg"
      }
   ],
   "autoload" : {
      "psr-4" : {
         "Enum\\" : "src/"
      }
   },
   "autoload-dev" : {
      "psr-4" : {
         "" : "tests/generated/"
      }
   },
   "bin" : [
      "bin/enum-generator"
   ],
   "description" : "Zero-dependency enumeration class code generator",
   "keywords" : [],
   "license" : "ISC",
   "minimum-stability" : "stable",
   "name" : "zurborg/enum-generator",
   "require" : {
      "php" : "^7.4",
      "zurborg/wrap-json" : "^1.0"
   },
   "require-dev" : {
      "phpunit/phpunit" : "~9",
      "squizlabs/php_codesniffer" : "~3"
   },
   "type" : "library"
}
